<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.budianto.mytourismapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyTourismApp">
        <activity android:name=".user.account.ForgetPasswordActivity"></activity>
        <activity android:name=".user.register.RegisterActivity" />
        <activity
            android:name=".user.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".detail.DetailTourismActivity"
            android:theme="@style/Theme.MyTourismApp" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.MyTourismApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>